{% extends "BackendBundle:Base:base.html.twig" %}

{#
    This is the regular template to extend.
    Contains regular header and footer.
#}

{% block header %}
    {% if not backend.security.logged %}
        {% include "BackendBundle:Include:notlogged_navigation.html.twig" %}
    {% else %}
        {% include "BackendBundle:Include:main_navigation.html.twig" %}
    {% endif %}
{% endblock %}

{% block footer %}
    {% include "BackendBundle:Include:footer.html.twig" %}
{% endblock %}
