{% extends "Base/default.html.twig" %}

{#
    This is the content-meta template to extend.
    Contains regular header, footer and a content and meta block
#}

{% block body %}

<div class="container-fluid">
    <div class="row-fluid">

        <div class="span3 meta-content">
        {% block meta_content %}
            <div class="well sidebar-nav">
                <ul class="nav nav-list">
                    <li class="nav-header">PivotX</li>
                    <li>
                        <p>
                            Put your own information here.
                        </p>
                    </li>
                </ul>
            </div>
        {% endblock %}
        </div><!--/span-->

        <div class="span9 content">
        {% block content %}
            9 columns of freedom
        {% endblock %}
        </div><!--/span-->

    </div><!--/row-->

{% endblock %}

